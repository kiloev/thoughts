{
  "headers": [
    {
      "source": "/sw.js",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)\\.(css|ico|jpeg|jpg|js|json|png)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=604800, stale-while-revalidate=2628000"
        }
      ]
    }
  ]
}
